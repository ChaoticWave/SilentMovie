{
    "name":              "chaoticwave/silent-movie",
    "description":       "Movie Data Analysis Tool",
    "keywords":          [
        "elasticsearch",
        "media",
        "movies",
        "analysis",
        "research"
    ],
    "license":           "MIT",
    "type":              "project",
    "require":           {
        "php":                         "^7.1.3",
        "ext-curl":                    "*",
        "ext-json":                    "*",
        "fideloper/proxy":             "^4.0",
        "laravel/framework":           "5.8.*",
        "laravel/tinker":              "^1.0",
        "chaoticwave/blue-velvet":     "1.1.*",
        "elasticsearch/elasticsearch": ">=2.0.0",
        "php-tmdb/api":                "~2.1",
        "php-tmdb/laravel":            "~1.1"
    },
    "require-dev":       {
        "filp/whoops":          "^2.0",
        "fzaninotto/faker":     "^1.4",
        "mockery/mockery":      "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit":      "^7.0"
    },
    "autoload":          {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4":    {
            "ChaoticWave\\SilentMovie\\": "app/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "ChaoticWave\\SilentMovie\\Tests\\": "tests/"
        }
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        },
        "laravel":      {
            "dont-discover": [
            ]
        }
    },
    "scripts":           {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd":   [
            "@php artisan key:generate"
        ],
        "post-autoload-dump":        [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config":            {
        "preferred-install":   "dist",
        "sort-packages":       true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable":     true
}
